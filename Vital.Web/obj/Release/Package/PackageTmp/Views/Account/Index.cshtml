                            <script type="text/javascript">

                                function submitRate() {

                                    var fromcountry = $("#fromcountry");
                                    var fromcity = $("#fromcity");
                                    var fromstate = $("#fromstate");

                                    var tocountry = $("#tocountry");
                                    var tocity = $("#tocity");
                                    var tostate = $("#tostate");

                                    var length = $("#length1");
                                    var width = $("#width1");
                                    var height = $("#height1");

                                    var commodity = $("#commodity");

                                    if (fromcity.val() == "") {
                                        alert("Please input city.");
                                        fromcity.focus();
                                    } else if (fromstate.val() == "") {
                                        alert("Please input state.");
                                        fromstate.focus();
                                    } else if (tocity.val() == "") {
                                        alert("Please input city.");
                                        tocity.focus();
                                    } else if (tostate.val() == "") {
                                        alert("Please input state.");
                                        tostate.focus();
                                    } else if (length.val() == "") {
                                        alert("Please input length.");
                                        length.focus();
                                    } else if (width.val() == "") {
                                        alert("Please input width.");
                                        width.focus();
                                    } else if (height.val() == "") {
                                        alert("Please input height.");
                                        height.focus();
                                    } else if (commodity.val() == "") {
                                        alert("Please input a commodity.");
                                        commodity.focus();
                                    } else {
                                        $("#loadingpanel").show();
                                        $("#resultrate").html("Please wait...");
                                        //var url = "http://localhost:60759/Account/CalculateRateLTL";
                                        var url = "http://86e5809092cb478994f1900a6012ac93.cloudapp.net/Account/CalculateRateLTL";
                                        //compute here
                                        $.post(url, {
                                            fromcountry: fromcountry.val(), fromcity: fromcity.val(), fromstate: fromstate.val(),
                                            tocountry: tocountry.val(), tocity: tocity.val(), tostate: tostate.val(), totalweight: $(".actualweight").text()
