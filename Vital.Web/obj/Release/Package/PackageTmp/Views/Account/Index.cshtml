                                                <script type="text/javascript">

                                                    function CalculateDimensionalWeight(e, type) {
                                                        $("#length").val($("#length1").val());
                                                        $("#width").val($("#width1").val());
                                                        $("#height").val($("#height1").val());
                                                        $("#weight").val($("#weight1").val());
                                                        

                                                        if (type == "skid1") {
                                                            var s1 = parseInt(($("#length1").val() * $("#width1").val() * $("#height1").val() / 1728) * 10) + 1;
                                                            $("#dweight1").val(s1);
                                                            
                                                        } else if (type == "skid2") {
                                                            var s1 = parseInt(($("#length2").val() * $("#width2").val() * $("#height2").val() / 1728) * 10) + 1;
                                                            $("#dweight2").val(s1);
                                                        } else if (type == "skid3") {
                                                            var s1 = parseInt(($("#length3").val() * $("#width3").val() * $("#height3").val() / 1728) * 10) + 1;
                                                            $("#dweight3").val(s1);
                                                        } else if (type == "skid4") {
                                                            var s1 = parseInt(($("#length4").val() * $("#width4").val() * $("#height4").val() / 1728) * 10) + 1;
                                                            $("#dweight4").val(s1);
                                                        }
                                                        GetActualWeight2();

                                                    }

                                                    function GetActualWeight() {
                                                        GetActualWeight2();
                                                    }

                                                    function GetActualWeight2() {

                                                        var getWeight1;
                                                        var getWeight2;
                                                        var getWeight3;
                                                        var getWeight4;

                                                        var dweight1 = $("#dweight1").val();;
                                                        var dweight2 = $("#dweight2").val();;
                                                        var dweight3 = $("#dweight3").val();;
                                                        var dweight4 = $("#dweight4").val();;

                                                        var weight1 = $("#weight1").val();;
                                                        var weight2 = $("#weight2").val();;
                                                        var weight3 = $("#weight3").val();;
                                                        var weight4 = $("#weight4").val();;

                                                        if (parseInt(dweight1) >= parseInt(weight1)) {
                                                            getWeight1 = parseInt(dweight1);
                                                        } else {
                                                            getWeight1 = parseInt(weight1);
                                                        }

                                                        if (parseInt(dweight2) >= parseInt(weight2)) {
                                                            getWeight2 = parseInt(dweight2);
                                                        } else {
                                                            getWeight2 = parseInt(weight2);
                                                        }

                                                        if (parseInt(dweight3) >= parseInt(weight3)) {
                                                            getWeight3 = parseInt(dweight3);
                                                        } else {
                                                            getWeight3 = parseInt(weight3);
                                                        }

                                                        if (parseInt(dweight4) >= parseInt(weight4)) {
                                                            getWeight4 = parseInt(dweight4);
                                                        } else {
                                                            getWeight4 = parseInt(weight4);
                                                        }

                                                        var totalweight = parseInt(getWeight1 + getWeight2 + getWeight3 + getWeight4);

                                                        $(".actualweight").html(totalweight);

                                                    }


                                                </script>
