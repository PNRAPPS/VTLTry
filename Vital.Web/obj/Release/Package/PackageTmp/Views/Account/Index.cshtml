                                                if (result.data > 0) {
                                                    $("#resultrate").html(result.data);
                                                } else {
                                                    $("#resultrate").html("No Rates Found.");
                                                }

                                            } else {

                                                $("#resultrate").html("No Rates Found.");
                                            }

                                        }, 'json');

                                    }

                                }

