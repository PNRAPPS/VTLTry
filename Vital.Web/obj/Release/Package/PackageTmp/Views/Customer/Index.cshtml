@{
    ViewBag.Title = "My Vital";
}

<div class="panel" style="background: #ededed; border: #ededed; margin-top: 0px; padding-top: 5px;">
    <div class="row">
        <div class="twelve columns">
            <h2 style="color: #f79020; margin-bottom: 25px;">My Vital</h2>
        </div>
    </div>
    <div class="row">
        <div class="twelve columns">
            <dl class="tabs" style="border-bottom: solid 1px #ee8f1e; margin-bottom: 0px;">
                <dd class="active"><a style="cursor: default; color: #ffffff; width: 120px; background: url('/Content/images/whitelockicon.png') no-repeat 78% #ee8f1e; border-left: 1px solid #ee8f1e; border-right: 1px solid #ee8f1e; font-weight: normal;">My Vital</a></dd>
            </dl>
            <ul class="tabs-content">
                <li class="active" id="simple1Tab">
                    <div class="panel" style="background: #fff; border: solid 1px #ee8f1e; margin: 0 0 22px 0; padding-top: 0px;">
                        <div class="row">
                            <div class="twelve columns">
                                <h5>My Vital - Main Page</h5>
                                <p>Welcome! My Vital includes maintenance of your shipping and tracking information.<br />Choose your activity below.</p>
                            </div>
                        </div>
                        <div class="row">
                            <div class="twelve columns">
                                <p style="margin-bottom: 5px;"><a class="myvitallnk" href="@Url.Content("~/Account/AccessAddressBook")">Access Address Book</a><br />Manage all your shipping addresses.</p>
                               <!-- <p style="margin-bottom: 5px;"><a class="myvitallnk" href="@Url.Content("~/Account/ChangePassword")">Change Password</a><br />Manage your password.</p>
                                   -->
                                <p style="margin-bottom: 5px;"><a class="myvitallnk" href="@Url.Content("~/Customer/ManageMyAccount")">Manage My Account</a><br />Manage your UPS Accounts and credit cards.</p>
                                <p style="margin-bottom: 5px;"><a class="myvitallnk" href="~/Account/UpdateMyProfile">Update My Profile</a><br />Manage your personal information.</p>
                                <p style="margin-bottom: 5px;"><a class="myvitallnk" href="~/Account/ManageInvoices" >View/Manage Invoices</a><br />Manage your shipment invoices.</p>
                                @*<p style="margin-bottom: 5px;"><a class="myvitallnk" href="#" onclick="clickUPS()">View/Manage Invoices</a><br />Manage your shipment invoices.</p>*@
                            </div>
                        </div>
                    </div>
                </li>
            </ul>
        </div>
    </div>
</div>
